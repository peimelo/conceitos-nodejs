{
  "name": "repositories-nodejs",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "concurrently \"cd backend && yarn install\" \"cd frontend && yarn install && yarn build\"",
    "deploy": "mkdir public && cp -a frontend/build/. public/",
    "postinstall": "yarn build && yarn deploy"
  },
  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
